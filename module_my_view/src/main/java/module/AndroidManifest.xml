<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kuaqu.reader.module_my_view">

    <application
        android:allowBackup="true"
        android:name=".app.MyAppView"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" />
    <activity android:name=".activity.ViewMainActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity android:name=".activity.StereActivity"></activity>
    <activity android:name=".activity.AnimationActivity" />
    <activity android:name=".activity.ProgressBarActivity" />
    <activity android:name=".activity.BitmapShaderActivity" />
    <activity android:name=".activity.SlantTvActivity" />
    <activity android:name=".activity.FoldTextActivity" />
    <activity android:name=".activity.PaintActivity" />
    <activity android:name=".activity.CustomViewActivity" />
    <activity android:name=".activity.BesselActivity" />
    <activity android:name=".activity.XFermodeActivity" />
    <activity android:name=".activity.TimerActivity" />
    <activity android:name=".activity.ClockActivity" />
    <meta-data
        android:name="design_width"
        android:value="1080"/>
    <meta-data
        android:name="design_height"
        android:value="1920"/>
</manifest>
